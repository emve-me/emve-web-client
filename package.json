{
  "name": "emve-client",
  "version": "1.0.0",
  "description": "emve.me, play youtube videos with friends.",
  "scripts": {
    "dev": "nodemon -e ts,tsx -w src --exec \"ts-node -r dotenv/config  src/server.ts\"",
    "clean": "rm -rf .next && rm -rf dist",
    "build": "NODE_ENV=production npm run clean && npm run build-client && npm run build-server",
    "build-client": "next build",
    "start": "NODE_ENV=production node dist/server.js",
    "start-spoof": "NODE_ENV=production node dist/server.js",
    "build-server": "tsc --build src/tsconfig.json",
    "gql-types": "apollo client:codegen --customScalarsPrefix csprefix --namespace dontseethenamespace --outputFlat --target typescript --includes=lib/**/*.tsx,lib/**/.ts gql_types"
  },
  "keywords": [],
  "author": "",
  "dependencies": {
    "@material-ui/core": "^3.9.3",
    "@types/dotenv": "^6.1.1",
    "@types/express": "^4.16.1",
    "@types/graphql": "^14.2.0",
    "@types/jsonwebtoken": "^8.3.2",
    "@types/next": "^8.0.4",
    "@types/node": "^10.14.6",
    "@types/react": "^16.8.14",
    "@types/react-dom": "^16.8.4",
    "@types/react-youtube": "^7.6.1",
    "@types/serve-favicon": "^2.2.30",
    "@types/styled-jsx": "^2.2.8",
    "@zeit/next-typescript": "^1.1.1",
    "apollo-cache-inmemory": "^1.5.1",
    "apollo-client": "^2.5.1",
    "apollo-link": "^1.2.11",
    "apollo-link-context": "^1.0.17",
    "apollo-link-http": "^1.5.14",
    "apollo-link-ws": "^1.0.17",
    "express": "^4.16.4",
    "gapi-to-graphql": "^2.0.5",
    "graphql": "^14.2.1",
    "graphql-tag": "^2.10.1",
    "jsonwebtoken": "^8.5.1",
    "next": "^8.1.0",
    "next-with-apollo": "^3.4.0",
    "react": "^16.8.6",
    "react-apollo": "^2.5.5",
    "react-dom": "^16.8.6",
    "react-google-login": "^5.0.4",
    "react-spring": "^8.0.19",
    "react-youtube": "^7.9.0",
    "serve-favicon": "^2.5.0",
    "subscriptions-transport-ws": "^0.9.16",
    "vanilla-cookies": "^0.0.7"
  },
  "devDependencies": {
    "apollo": "^2.8.3",
    "dotenv": "^7.0.0",
    "nodemon": "^1.18.11",
    "prettier": "^1.17.0",
    "ts-node": "^8.1.0",
    "typescript": "^3.4.5"
  },
  "engines": {
    "node": "10.x.x",
    "npm": "6.9.0"
  }
}

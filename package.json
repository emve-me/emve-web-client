{
  "name": "emve-client",
  "version": "1.0.0",
  "description": "emve.me, play youtube videos with friends.",
  "scripts": {
    "dev": "nodemon -e ts,tsx -w src --exec \"ts-node -r dotenv/config  src/server.ts\"",
    "clean": "rm -rf .next && rm -rf dist",
    "build": "NODE_ENV=production npm run clean && npm run build-client && npm run build-server",
    "build-client": "next build",
    "start": "NODE_ENV=production node dist/server.js",
    "start-spoof": "NODE_ENV=production node dist/server.js",
    "build-server": "tsc --build src/tsconfig.json",
    "gql-types": "rm -rf gql_types && apollo client:codegen --customScalarsPrefix csprefix --namespace dontseethenamespace --outputFlat --target typescript --includes=lib/**/*.tsx,lib/**/.ts gql_types"
  },
  "keywords": [],
  "author": "",
  "dependencies": {
    "@zeit/next-typescript": "^1.1.1",
    "apollo-cache-inmemory": "^1.6.3",
    "apollo-client": "^2.6.4",
    "apollo-link": "^1.2.13",
    "apollo-link-context": "^1.0.19",
    "apollo-link-http": "^1.5.16",
    "apollo-link-ws": "^1.0.19",
    "copy-to-clipboard": "^3.2.0",
    "express": "^4.17.1",
    "gapi-to-graphql": "^2.0.7",
    "graphql": "^14.5.8",
    "graphql-tag": "^2.10.1",
    "next": "8.1.0",
    "next-with-apollo": "^4.3.0",
    "react": "^16.12.0",
    "react-apollo": "^3.1.3",
    "react-dom": "^16.12.0",
    "react-google-login": "^5.0.7",
    "react-spring": "^8.0.27",
    "react-youtube": "^7.9.0",
    "serve-favicon": "^2.5.0",
    "subscriptions-transport-ws": "^0.9.16",
    "vanilla-cookies": "^0.0.8"
  },
  "devDependencies": {
    "@types/dotenv": "^8.2.0",
    "@types/express": "^4.17.2",
    "@types/graphql": "^14.5.0",
    "@types/next": "8.0.5",
    "@types/node": "^10.17.9",
    "@types/react": "^16.9.16",
    "@types/react-dom": "^16.9.4",
    "@types/react-youtube": "^7.6.2",
    "@types/serve-favicon": "^2.5.0",
    "@types/styled-jsx": "^2.2.8",
    "apollo": "^2.21.2",
    "dotenv": "^8.2.0",
    "husky": "^2.7.0",
    "nodemon": "^2.0.2",
    "prettier": "^1.19.1",
    "pretty-quick": "^2.0.1",
    "ts-node": "^8.5.4",
    "typescript": "^3.7.3"
  },
  "engines": {
    "node": "12.x.x",
    "npm": "6.13.3"
  },
  "husky": {
    "hooks": {
      "pre-commit": "pretty-quick --staged"
    }
  }
}
